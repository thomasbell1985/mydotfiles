# Source the configurations
source $HOME/code/mydotfiles/zsh/core.zsh
source $HOME/code/mydotfiles/zsh/prompt.zsh
source $HOME/code/mydotfiles/zsh/aliases.zsh
source $HOME/code/mydotfiles/zsh/functions.zsh
source $HOME/code/mydotfiles/zsh/completion.zsh
source $HOME/code/mydotfiles/zsh/environment.zsh
source $HOME/code/mydotfiles/zsh/plugins.zsh
source $HOME/code/mydotfiles/zsh/gcloud.zsh

echo "source of zsh file completed"



# The next line updates PATH for the Google Cloud SDK.
if [ -f '/home/thomasbell/code/mydotfiles/utilities/google-cloud-sdk/path.zsh.inc' ]; then . '/home/thomasbell/code/mydotfiles/utilities/google-cloud-sdk/path.zsh.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/home/thomasbell/code/mydotfiles/utilities/google-cloud-sdk/completion.zsh.inc' ]; then . '/home/thomasbell/code/mydotfiles/utilities/google-cloud-sdk/completion.zsh.inc'; fi
